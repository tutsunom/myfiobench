[global]
ioengine=libaio
blocksize=<BLOCKSIZE>
blockalign=<BLOCKALIGN>
kb_base=1024
#filesize=1-100GB
size=100%
direct=1
ioscheduler=<IOSCHED>
readwrite=<READWRITE>
filename=<FILENAME>
ramp_time=<RAMPTIME>
runtime=<RUNTIME>
time_based=1
group_reporting=1

[<FILENAME>_<IOSCHED>_<READWRITE>_<BLOCKSIZE>_<NUMJOBS>jobs_<IODEPTH>depth_<RUNTIME>sec]
numjobs=<NUMJOBS>
iodepth=<IODEPTH>
